<h1>All Questions</h1>
<a class="btn btn-success" href="/questions/new">Create a New Question</a>
 <% @questions.each do |question| %>
   <div class="media">
     <div class="media-body">
       <h4 class="media-heading">
         <%= 'R ' if question.resolved %>
         <%= 'U ' if !question.resolved %>
         <%= link_to question.title, question %>
       </h4>
     </div>
   </div>
 <% end %>